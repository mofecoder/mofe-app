datasource db {
  provider = "mysql"
  url      = env("API_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contest {
  id Int @id @default(autoincrement())
  slug String
  name String
  description String
  kind String
  penaltyTime Int @default(0)
  startAt DateTime
  endAt DateTime?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Task {
  id    Int     @id @default(autoincrement())
  label String
  done  Boolean @default(false)
}
